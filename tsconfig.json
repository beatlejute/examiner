{
    "include": [
        "./src/**/*",
        "./types.d.ts"
    ],
    "compileOnSave": true,
    "compilerOptions": {
        "importHelpers": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "strict": true,
        "noUnusedLocals": false,
        "baseUrl": ".",
        "outDir": "./SBIS.Webinar",
        "lib": ["dom", "es5", "es2015", "scripthost"],
        "module": "amd",
        "moduleResolution": "classic",
        "target": "es5",
        "typeRoots": [
            "./node_modules/@types",
            "./types.d.ts"
        ],
        "paths": {
            "VideoCall/*": [
                "../../../videocall/cloud/client/VideoCall/*"
            ],
            "SBIS.VC/*": [
                "../../../videocall/cloud/client/SBIS.VC/*"
            ],
            "tslib" : [
                "./node_modules/tslib/tslib.d.ts"
            ]
        }
    }
}
